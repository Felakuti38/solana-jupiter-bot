# Solana wallet private key (base58 encoded secret key)
SOLANA_WALLET_PRIVATE_KEY=

# Optional: public key for validation
SOLANA_WALLET_PUBLIC_KEY=

# Default RPC endpoint (use a reliable provider)
DEFAULT_RPC=https://api.mainnet-beta.solana.com

# Alternative RPC list (comma-separated)
ALT_RPC_LIST=https://solana.drpc.org,https://solana-mainnet.chainstacklabs.com

# Skip intro screen
SKIP_INTRO=true

# Enable extra logging
DEBUG=false
# Solana Jupiter Bot Configuration
# Copy this file to .env and fill in your values

# Required: Your wallet private key (base58 encoded)
SOLANA_WALLET_PRIVATE_KEY=your_private_key_here

# Required: Primary RPC endpoint (will be selected by default)
# Choose from the available RPCs below or use your own
DEFAULT_RPC=https://api.mainnet-beta.solana.com

# Optional: Additional custom RPC endpoints (comma-separated)
# These will be added to the RPC selection list
ALT_RPC_LIST=https://solana-api.projectserum.com,https://rpc.ankr.com/solana

# Optional: Skip intro screen
SKIP_INTRO=false

