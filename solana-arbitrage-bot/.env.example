# Solana Configuration
SOLANA_RPC_ENDPOINT=https://api.mainnet-beta.solana.com
SOLANA_WS_ENDPOINT=wss://api.mainnet-beta.solana.com

# Wallet Configuration (NEVER commit real private keys!)
WALLET_PRIVATE_KEY=your_wallet_private_key_here

# Trading Configuration
MIN_PROFIT_THRESHOLD=0.005  # Minimum 0.5% profit to execute trade
MAX_SLIPPAGE=0.02           # Maximum 2% slippage allowed
MAX_POSITION_SIZE=100       # Maximum position size in SOL
MIN_LIQUIDITY=1000          # Minimum liquidity in USD

# Risk Management
MAX_DAILY_LOSS=100          # Maximum daily loss in SOL
MAX_TRADES_PER_MINUTE=10    # Rate limiting
STOP_LOSS_PERCENTAGE=0.05   # 5% stop loss

# DEX Configuration
ENABLE_RAYDIUM=true
ENABLE_ORCA=true
ENABLE_JUPITER=true

# Monitoring
MONITORING_PORT=3000
ENABLE_DASHBOARD=true
LOG_LEVEL=info

# Meme Coin Filters
MIN_MARKET_CAP=10000        # Minimum market cap in USD
MAX_MARKET_CAP=10000000     # Maximum market cap in USD
MIN_VOLUME_24H=1000         # Minimum 24h volume in USD